<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lzy.pojo">
	<class name="Apparatus" table="a_apparatus">
		<id name="a_id">
			<generator class="native"></generator>
		</id>
		<property name="a_name"></property>
		<property name="a_state"></property>
		<property name="a_time"></property>
		<property name="a_manufacturer"></property>
	</class>
</hibernate-mapping>