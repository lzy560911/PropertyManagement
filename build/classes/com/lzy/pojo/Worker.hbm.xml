<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lzy.pojo">
	<class name="Worker" table="a_worker">
		<id name="w_id">
			<generator class="native"></generator>
		</id>
		<property name="w_name"></property>
		<property name="w_sex"></property>
		<property name="w_cardnumber"></property>
		<property name="w_phone"></property>
		<property name="d_name"></property>
	</class>
</hibernate-mapping>