<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lzy.pojo">
	<class name="Administrators" table="a_user">
		<id name="A_id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="A_name"></property>
		<property name="A_password"></property>
		<property name="A_phone"></property>
		<property name="A_security"></property>
	</class>
</hibernate-mapping>