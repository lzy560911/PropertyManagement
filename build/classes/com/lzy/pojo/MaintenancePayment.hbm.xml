<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lzy.pojo">
	<class name="MaintenancePayment" table="a_maintenance_payment">
		<id name="m_id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="h_house_number"></property>
		<property name="m_name"></property>
		<property name="m_phone"></property>
		<property name="m_work"></property>
		<property name="m_money"></property>
		<property name="m_time"></property>
	</class>
</hibernate-mapping>