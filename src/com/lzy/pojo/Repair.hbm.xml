<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lzy.pojo">
	<class name="Repair" table="a_repair">
		<id name="r_id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="h_house_number"></property>
		<property name="r_name"></property>
		<property name="r_phone"></property>
		<property name="r_message"></property>
		<property name="r_time"></property>
		<property name="r_state"></property>
	</class>
</hibernate-mapping>